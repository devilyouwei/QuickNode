{
    "name": "quicknode",
    "version": "1.1.6",
    "author": "devilyouwei",
    "keywords": [
        "quicknode",
        "nodejs",
        "RESTful",
        "API",
        "web framework",
        "Convention Routing",
        "MVC"
    ],
    "description": "A simple and easy way for you to create RESTful API on Node.js.",
    "main": "server.js",
    "dependencies": {
        "express": "latest",
        "mysql2": "latest",
        "connect-multiparty": "latest",
        "trim": "latest",
        "htmlspecialchars": "latest",
        "md5": "latest",
        "superagent": "latest",
        "app-root-path": "latest"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "prettier": "^1.19.1"
    },
    "eslintConfig": {
        "root": true,
        "parserOptions": {
            "ecmaVersion": 2017,
            "parser": "babel-eslint"
        },
        "env": {
            "es6": true,
            "node": true
        },
        "plugins": [
            "prettier"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:prettier/recommended"
        ],
        "rules": {
            "no-console": "off",
            "no-unused-vars": "warn",
            "prettier/prettier": [
                "error"
            ]
        }
    },
    "prettier": {
        "trailingComma": "none",
        "printWidth": 120,
        "tabWidth": 4,
        "semi": false,
        "bracketSpacing": true,
        "htmlWhitespaceSensitivity": "ignore",
        "arrowParens": "avoid",
        "singleQuote": true,
        "endOfLine": "auto"
    },
    "license": "MIT"
}
